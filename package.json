{
  "name": "ucf-course-api",
  "description": "An unofficial API for UCF's courses",
  "version": "1.0.0",
  "main": "server.js",
  "license": "MIT",
  "scripts": {
    "build": "babel ./src -d dist --ignore src/__tests__/**/* --extensions \".ts,.js\"",
    "dev": "ts-node-dev ./src/server.ts | pino-pretty",
    "lint": "eslint './src/**/*{.js,.ts}'",
    "start": "node ./dist/server.js | pino-pretty",
    "test": "jest --bail --passWithNoTests --coverage"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "@types/node-fetch": "^2.5.10",
    "cheerio": "^1.0.0-rc.6",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "node-fetch": "^2.6.1",
    "pino": "^6.11.3",
    "pino-pretty": "^4.7.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/node": "^7.13.13",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-typescript": "^7.13.0",
    "@types/cheerio": "^0.22.28",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/node": "^15.0.2",
    "@types/pino": "^6.3.8",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.3.6",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*{.js,.ts}": [
      "prettier --write",
      "yarn lint",
      "yarn test"
    ]
  },
  "engines": {
    "node": "14.x",
    "yarn": "1.x"
  }
}
